    function seisTeleop() {
        let x = document.getElementById('siesmometer');
        if (x.value == 0) {
            total += seisTelePoint;
        } else if (x.value == seisAutoPoint) {
            total -= seisAutoPoint;
            total += seisTelePoint;
        }

        x.value = seisTelePoint;
        //let y = document.getElementById('total').value
        document.getElementById('current-seis-points').innerHTML = 'Current Points: ' + seisTelePoint;
        document.getElementById('total').value = total;
        document.getElementById('totalPoints').innerHTML = 'Total Points: ' + total;
        document.getElementById('scoreTeams').submit();
        //console.log(x.value);
        //return false;
        //seisPoints = 10;
        //document.getElementById('test').innerHTML = document.getElementById('<%=seis.ClientID %>').value;
        //console.log(document.getElementById('seis').value);
    }

