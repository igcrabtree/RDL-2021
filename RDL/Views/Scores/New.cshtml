        if (autoImagesScored > 0) {
            autoImagesScored--;
            imagesScored--;
            let x = document.getElementById('imageRecog');
            currentImagePoints -= imageAutoPoint;
            x.value = currentImagePoints;
            document.getElementById('current-image-points').innerHTML = 'Current Points: ' + currentImagePoints;
            document.getElementById('auto-images').innerHTML = 'Images Recognized Autonomously: ' + autoImagesScored;
