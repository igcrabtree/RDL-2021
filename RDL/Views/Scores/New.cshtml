    function imageTeleop() {
        if (imagesScored < 3) {
            teleImagesScored++;
            let x = document.getElementById('imageRecog');
            currentImagePoints += imageTelePoint;
            x.value = currentImagePoints;
            imagesScored++;
            document.getElementById('current-image-points').innerHTML = 'Current Points: ' + currentImagePoints;
